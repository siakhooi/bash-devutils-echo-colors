#!/bin/bash

PROGRESS_BAR_DONE_CHAR=$(echo.green -n "▇")
PROGRESS_BAR_LEFT_CHAR=$(echo.grey -n "▇")
PROGRESS_BAR_OPEN_BRACKET=$(echo.grey -n "")
PROGRESS_BAR_CLOSE_BRACKET=$(echo.grey -n "")
export PROGRESS_BAR_DONE_CHAR PROGRESS_BAR_LEFT_CHAR PROGRESS_BAR_OPEN_BRACKET PROGRESS_BAR_CLOSE_BRACKET

print-progress "$@"
